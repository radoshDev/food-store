---
import data from '@/data/data.json'
import ServicesListCard from './ServicesListCard.astro'
---

<div class="services_list">
	{data.services.map(service => <ServicesListCard {...service} />)}
</div>

<style lang="scss">
	.services_list {
		display: flex;
		gap: 3rem;
		justify-content: center;
		padding: 0 2.6rem;
		margin-bottom: 7.6rem;
	}
</style>
