---
type Props = {
	imageName: string
	alt: string
	width: number
	height: number
}

const { alt, imageName, height, width } = Astro.props
---

<picture>
	<source type="image/avif" srcset={`assets/${imageName}.avif`} />
	<source type="image/webp" srcset={`assets/${imageName}.webp`} />
	<img
		width={width}
		height={height}
		loading="lazy"
		decoding="async"
		src={`assets/${imageName}.jpg`}
		alt={alt}
	/>
</picture>
