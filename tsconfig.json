{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": ".",
		"paths": {
			"@ui/*": ["src/components/ui/*"],
			"@components/*": ["src/components/*"],
			"@assets/*": ["src/assets/*"],
			"@data/*": ["src/data/*"],
			"@styles/*": ["src/styles/*"]
		},
		"types": ["@astrojs/image/client"]
	}
}
